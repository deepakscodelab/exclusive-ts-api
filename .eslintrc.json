{
  "extends": "airbnb",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": "off",
    "import/newline-after-import": "off",
    "no-unused-vars": 0,
    "max-len": 0,
    "linebreak-style": 0,
    "comma-dangle": [
      "error",
      "never"
    ]
    // ,
    // "space-before-function-paren": [
    //   "error",
    //   {
    //     "anonymous": "always",
    //     "named": "always",
    //     "asyncArrow": "always"
    //   }
    // ]
  }
}